{
  "name": "vue-occasions",
  "private": false,
  "version": "1.0.1",
  "keywords": ["dates", "holidays", "occasions"],
  "main": "./dist/umd.js",
  "module": "./dist/es.js",
  "exports": {
    ".": {
      "import": "./dist/es.js",
      "require": "./dist/umd.js"
    },
    "./dist/occasions.json": "./dist/occasions.json"
  },
  "files": [
    "dist"
  ],
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "test": "vitest",
    "coverage": "vitest run --coverage"
  },
  "dependencies": {
    "vue": "^3.2.47"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^4.1.0",
    "npmignore": "^0.3.0",
    "vite": "^4.2.0",
    "vitest": "^0.29.8"
  },
  "publishConfig": {
    "ignore": [
      "!build/",
      "src/",
      "tests/"
    ]
  }
}
