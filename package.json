{
  "name": "vue-occasions",
  "description": "Tags your HTML’s body element with a class and data attribute reflecting today’s occasion or holiday.",
  "private": false,
  "version": "1.1.0",
  "keywords": [
    "dates",
    "holidays",
    "occasions"
  ],
  "main": "./dist/umd.js",
  "module": "./dist/es.js",
  "exports": {
    ".": {
      "import": "./dist/es.js",
      "require": "./dist/umd.js"
    },
    "./dist/occasions.json": "./dist/occasions.json"
  },
  "files": [
    "./dist",
    "./src/plugins/VueOccasions/occasions.json"
  ],
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "test": "vitest",
    "coverage": "vitest run --coverage"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/speasley/vue-occasions.git"
  },
  "author": "Stephen Peasley",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/speasley/vue-occasions/issues"
  },
  "homepage": "https://github.com/speasley/vue-occasions/#readme",
  "dependencies": {
    "vue": "^3.2.47"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^4.1.0",
    "@vitest/coverage-istanbul": "^0.29.8",
    "npmignore": "^0.3.0",
    "vite": "^4.2.0",
    "vitest": "^0.29.8"
  },
  "publishConfig": {
    "ignore": [
      "!build/",
      "src/",
      "tests/"
    ]
  }
}
